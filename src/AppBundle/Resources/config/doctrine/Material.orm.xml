    <field name="type" type="string" column="type" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="stockunit" type="string" column="stockunit" length="10" nullable="true">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="priceunit" type="string" column="priceunit" length="10" nullable="true">
